	},
	"dependencies": {
		"@googleworkspace/drive-picker-element": "workspace:*"
	},
	"devDependencies": {
		"@testing-library/react": "^16.3.0",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"@vitest/coverage-v8": "^4.0.10",
		"jsdom": "^27.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tsup": "catalog:",
		"typescript": "catalog:",
		"vitest": "^4.0.10"
