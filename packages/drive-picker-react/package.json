{
	"name": "@googleworkspace/drive-picker-react",
	"version": "0.0.0",
	"description": "React wrapper for Google Drive Picker Element",
	"license": "Apache-2.0",
	"author": {
		"name": "Justin Poehnelt",
		"email": "jpoehnelt@google.com",
		"url": "https://justin.poehnelt.com"
	},
	"keywords": [
		"google",
		"drive",
		"picker",
		"react",
		"drive-picker"
	],
	"type": "module",
	"main": "./dist/index.js",
