{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "custom-elements.json"]
		},
		"test": {
			"dependsOn": ["build"],
			"inputs": ["src/**/*.ts", "test/**/*.ts"]
		},
		"lint": {
			"outputs": []
		},
		"format": {
			"outputs": []
		},
		"check": {
			"dependsOn": ["build"],
			"outputs": []
		}
	}
}
