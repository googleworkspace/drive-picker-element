{
	"name": "@googleworkspace/drive-picker-element",
	"version": "0.2.1",
	"description": "A web component for Google Drive Picker",
	"license": "Apache-2.0",
	"author": {
		"name": "Justin Poehnelt",
		"email": "jpoehnelt@google.com",
		"url": "https://justin.poehnelt.com"
	},
	"type": "module",
	"exports": {
		".": {
			"browser": "./dist/index.iife.min.js",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./drive-picker": {
			"types": "./dist/drive-picker/index.d.ts",
			"import": "./dist/drive-picker/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src",
		"custom-elements.json"
	],
	"homepage": "https://github.com/googleworkspace/drive-picker-element",
	"repository": {
		"type": "git",
		"url": "https://github.com/googleworkspace/drive-picker-element.git"
	},
	"scripts": {
		"build": "wireit",
		"check": "tsc --noEmit",
		"format": "biome check --fix .",
		"lint": "biome check .",
		"prepack": "pnpm build",
		"prepare": "husky",
		"storybook": "wireit",
		"test": "echo \"No tests yet.\""
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@custom-elements-manifest/analyzer": "^0.10.2",
		"@storybook/addon-essentials": "^8.4.4",
		"@storybook/blocks": "^8.4.4",
		"@storybook/csf": "^0.1.11",
		"@storybook/types": "^8.4.4",
		"@storybook/web-components": "^8.4.4",
		"@storybook/web-components-vite": "^8.4.4",
		"@types/gapi": "^0.0.47",
		"@types/google.accounts": "^0.0.14",
		"@types/google.picker": "^0.0.46",
		"@types/mdast": "^4.0.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^22.4.1",
		"@types/unist": "^3.0.3",
		"browserslist": "^4.24.0",
		"browserslist-to-esbuild": "^2.1.1",
		"custom-elements-manifest": "^2.1.0",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.9",
		"mdast-util-from-markdown": "^2.0.2",
		"prettier": "^3.3.3",
		"remark": "^15.0.1",
		"remark-behead": "^3.1.0",
		"remark-gfm": "^4.0.0",
		"remark-github": "^12.0.0",
		"remark-toc": "^9.0.0",
		"storybook": "^8.4.4",
		"tsup": "^8.0.2",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"wireit": "^0.14.4"
	},
	"customElements": "dist/custom-elements.json",
	"wireit": {
		"build": {
			"dependencies": [
				"build:components",
				"build:manifest",
				"build:readme",
				"build:storybook"
			]
		},
		"build:components": {
			"command": "tsup",
			"files": [
				"src/**/*.ts",
				"tsup.config.ts"
			],
			"output": [
				"dist/**/*.(js|ts|map)"
			]
		},
		"build:manifest": {
			"command": "cem analyze --config cem.config.js && biome check --fix custom-elements.json",
			"files": [
				"src/**/*.ts",
				"cem.config.js"
			],
			"output": [
				"custom-elements.json"
			]
		},
		"build:readme": {
			"clean": false,
			"command": "tsx ./scripts/docs.ts",
			"files": [
				"custom-elements.json",
				"scripts/**/*.ts",
				"README.md"
			],
			"output": [
				"README.md"
			],
			"dependencies": [
				"build:manifest"
			]
		},
		"build:storybook": {
			"command": "storybook build",
			"files": [
				"src/**/*.ts",
				"stories/**/*.ts"
			],
			"output": [
				"dist/storybook"
			]
		},
		"storybook": {
			"service": true,
			"command": "storybook dev -p 6006",
			"dependencies": [
				"build:manifest"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "pnpm build && pnpm format && pnpm lint && pnpm check"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://wombat-dressing-room.appspot.com"
	},
	"packageManager": "pnpm@9.3.0"
}
